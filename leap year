#include <iostream>
using namespace std;


int main()
{
    int y;
    cin >> y;
    if (y <= 0)
    {
        cout << "m=null";
        return 0;
    }
    else
    {
        if (y % 4 == 0 && y % 100 != 0 || y % 400 == 0)
            cout << "该年份是闰年\n";
        else
            cout << "该年份不是闰年\n";
        cout << "\n";
    }
    return 0;

}
