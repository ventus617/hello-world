#include<bits/stdc++.h>                       //万能头文件
using namespace std;

int main()
{
    int a, b, c,max;
    printf("Please Enter 3 integers\n");
    (void)scanf_s("%d,%d,%d", &a, &b, &c);
    if (a > b&&a>c)
    {
        max = a;
    }
    else
        if (b > c)
            max = b;
        else 
            max = c;
    printf("The max is=%d", max);
     return 0;
}
