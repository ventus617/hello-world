#include<bits/stdc++.h>                       //万能头文件
using namespace std;

int main()
{   
    double grade;
    int t;
    printf("Please Enter your grade\n");
    cin >> grade;
    t = grade / 10;
    switch (t)
    {
    case 1:
    case 2:
    case 3:
    case 4:
    case 5:cout << "Your grade is E";
        break;
    case 6:cout << "Your grade is D";
        break;
    case 7:cout << "Your grade is C";
        break;
    case 8:cout << "Your grade is B";
        break;
    case 9:
    case 10:cout <<"Your grade is A";
        break;
    default:cout << "Null";
    }

    return 0;
}
