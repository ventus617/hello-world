#include<bits/stdc++.h>                       //万能头文件
using namespace std;

int main()
{   
	cout << "请输入：\n";
	int c;
	cin >> c;
	switch (c)
	{
	case 1:cout << "Monday";
		break;
	case 2:cout << "Tuesday";
		break;
	case 3:cout << "Wednesday";
		break;
	case 4:cout << "Thursday";
		break;
	case 5:cout << "Friday";
		break;
	case 6:cout << "Saturday";
		break;
	case 7:cout << "Sunday";
		break;
	default:cout << "Error input!";
		break;
	}
	return 0;
}
